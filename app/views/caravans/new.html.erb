<div class="container">
<%= simple_form_for(@caravan) do |f| %>
<%= f.input :brand %>
<%= f.input :year %>
<%= f.input :model %>
<%= f.input :capacity %>
<%= f.input :gas_type %>
<%= f.input :address %>
<%= f.input :description %>
<%= f.input :day_price %>
<%= f.input :photo, as: :file %>
<%= f.submit %>
<% end %>
</div>
